<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Picoage.Logger</id>
    <version>1.0.1</version>
    <description>
      This package can be used to log structure logs to application insight.  
    </description>
      <authors>Vishnu - Picoage</authors>
    <license type="expression">Apache-2.0</license>
      <!-- Dependencies are automatically installed when the package is installed -->
        <dependencies>
            <dependency id="Microsoft.Extensions.Logging.Abstractions" version="6.03" />
            <dependency id="Microsoft.ApplicationInsights.AspNetCore" version="2.21.0" />
        </dependencies>
       </metadata>
  <files>
    <file src="src\**\*.*" exclude="Src\**\bin\**\*.*;Src\**\obj\**\*.*" target="src" />
    <file src=".\README.md" target="" />
  </files>  
</package>